{
  "manifest_version": 3,
  "name": "BiliBlock-通过关键词屏蔽B站视频",
  "version": "1.1",
  "description": "屏蔽B站视频，按照标题关键词或分区关键词定向屏蔽视频",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://*.bilibili.com/*"
  ],
  "web_accessible_resources": [{
    "resources": ["tidmap.json"],
    "matches": ["https://*.bilibili.com/*"]
  }],
  "content_scripts": [
    {
      "matches": [
        "https://*.bilibili.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "128.png",
      "48": "128.png",
      "128": "128.png"
    }
  },
  "icons": {
    "16": "128.png",
    "48": "128.png",
    "128": "128.png"
  }
}