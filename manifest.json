{
  "manifest_version": 3,
  "name": "BiliBlock-通过关键词屏蔽B站视频",
  "version": "1.0",
  "description": "屏蔽B站视频，只要标题中包含关键词的视频都会被屏蔽，目前支持了首页和热门页视频。",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}